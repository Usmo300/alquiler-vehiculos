<form action="/rentalCars/createCar" method="POST" id="formRent">
        
        <h2>Coche nuevo para alquiler</h2>

        <section id="">
            <label for="carBrand">Marca: </label>
            <input type="text" id="carBrand" name="carBrand" placeholder="Marca"/>
        </section>

        <section id="">
            <label for="carModel">Modelo del coche: </label>
            <input type="text" id="carModel" name="carModel" placeholder="Modelo"/>
        </section>

        <section id="">
            <label for="modelYear">Año del vehículo: </label>
            <input type="text" id="modelYear" name="modelYear" placeholder="modelYear"/>
        </section>

        <section id="nextItvDate">
            <label for="nextItvDate">Próxima ITV: </label>
            <input type="date" id="nextItvDate" name="nextItvDate" placeholder="fecha ITV"/>
        </section>

        <section id="sellingPrice">
            <label for="sellingPrice">Precio de venta: </label>
            <input type="number" id="sellingPrice" name="sellingPrice" placeholder="Precio de venta"/>
        </section>

        <section id="costPrice">
            <label for="costPrice">Precio de coste del producto: </label>
            <input type="number" id="costPrice" name="costPrice" placeholder="Precio del coste del producto"/>
        </section>

        <section id="leasingPrice">
            <label for="leasingPrice">Precio alquiler: </label>
            <input type="number" id="leasingPrice" name="leasingPrice" placeholder="Precio alquiler"/>
        </section>

        <section id="leaseDate">
            <label for="leaseDate">Fecha de alquiler: </label>
            <input type="date" id="leaseDate" name="leaseDate" placeholder="Fecha de alquiler"> 
        </section>

        <section id="leaseReturnDate">
            <label for="leaseReturnDate">Fecha devolución: </label>
            <input type="date" id="leaseReturnDate" name="leaseReturnDate" placeholder="Fecha devolución" >
        </section>

        <section id="carImage">
            <label for="carImage">Imagen: </label>
            <input type="text" id="carImage" name="carImage" placeholder="Imagen">
        </section>
        
        <section id="carColor">
            <label for="carColor">Color del coche: </label>
            <input type="text" id="carColor" name="carColor" placeholder="Color">
        </section>
        
        <section id="seatsNumber">
            <label for="seatsNumber">Número de asientos: </label>
            <input type="number" id="seatsNumber" name="seatsNumber" placeholder="Nº de asientos">
        </section>

        <section id="doorNumber">
            <label for="doorNumber">Número de puertas: </label>
            <input type="number" id="doorNumber" name="doorNumber" placeholder="Nº de Puertas">
        </section>

        <section id="trunkSize">
            <label for="trunkSize">Tamaño maletero: </label>
            <input type="number" id="trunkSize" name="trunkSize" placeholder="Tamaño maletero">
        </section>
        
        <section id="co2Emissions">
            <label for="co2Emissions">co2 Emisiones: </label>
            <input type="text" id="co2Emissions" name="co2Emissions" placeholder="co2 emisiones">
        </section>
        
        <section id="carType">
            <label for="carType">Tipo de coche: </label>
            <input type="text" id="carType" name="carType" placeholder="Tipo de coche">
        </section>

        <section id="airConditioner">
            <label for="airConditioner">Aire acondicionado: </label>
            <input type="text" id="airConditioner" name="airConditioner" placeholder="Aire acondicionado">
        </section>

        <section id="transmissionType">
            <label for="transmissionType">Tipo de transmisión: </label>
            <input type="text" id="transmissionType" name="transmissionType" placeholder="Tipo de transmisión">
        </section>
        
        <section id="motorType">
            <label for="motorType">Tipo de motor: </label>
            <input type="text" id="motorType" name="motorType" placeholder="Tipo de motor">
        </section>
        
        <section id="stock">
            <label for="stock">Stock: </label>
            <input type="number" id="stock" name="stock" placeholder="stock">
        </section>
        
        <section id="availability">
            <label for="availability">Disponibilidad: </label>
            <input type="text" id="availability" name="availability" placeholder="Disponibilidad">
        </section>
        
        <button type="submit">Enviar</button>
        <button type="reset">Reset</button>

    </form>